package components

templ Page(treeData, nodesData, registeredTools string) {
	<!DOCTYPE html>
	<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>Agentize Knowledge Tree Documentation</title>
		@styles()
	</head>
	<body>
		<div class="container">
			@header()
			@stats()
			<div class="content">
				@treeView()
				@detailView()
				@toolsView()
			</div>
		</div>
		@scripts(treeData, nodesData, registeredTools)
	</body>
	</html>
}

